<!doctype html>
<html lang="en">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <!-- Bootstrap CSS -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">

  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css"
    integrity="sha512-1ycn6IcaQQ40/MKBW2W4Rhis/DbILU74C1vSrLJxCq57o941Ym01SwNsOMqvEBFlcgUa6xLiPY/NS5R+E6ztJQ=="
    crossorigin="anonymous" referrerpolicy="no-referrer" />

  <link rel="stylesheet" href="/static/css/style.css">
  <title>Dynamic web</title>
</head>

<body>
  {{> navbar data=details}}

  {{> slider slides=slides}}


  {{!-- banner1 section --}}

  <section class="banner1_section background1 py-5">
    <div class="container text-center">
      <div class="row">
        <div class="col-md-12">
          <h2>Start learning new technology here !!</h2>
          <p>
            Lorem ipsum dolor sit, amet consectetur adipisicing elit. Mollitia, quidem.
          </p>
          <button class="btn btn-outline-primary">Start learning</button>
        </div>
      </div>
    </div>
  </section>



  {{!-- services section --}}

  <section id="services_section" class="py-4">
    <div class="container">
      <h1 class="text-center text-secondary">Services that we provid</h1>

      <div class="row mt-5">
        {{!-- First service colum --}}

        {{#each services}}
        <div class="col-md-4">
          <div class="card">
            <div class="card-body">
              <div class="container text-center py-3">
                <i class="{{this.icon}} service_icon text-secondary"></i>
              </div>
              <h5 class="card-title">{{this.title}}</h5>
              <p>{{this.description}}</p>

              <a href="{{this.link}}">{{this.linkText}}</a>
              <a href="#!" class="ml-3">Support</a>
            </div>
          </div>
        </div>

        {{!-- <div class="col-md-4">
          <div class="card">
            <div class="card-body">
              <div class="container text-center py-3">
                <i class="fab fa-accusoft service_icon text-secondary"></i>
              </div>
              <h5 class="card-title">Provide Best Courses</h5>
              <p>We provide courses that helps our student in learning and in placement</p>

              <a href="#!">Check</a>
              <a href="#!" class="ml-3">Support</a>
            </div>
          </div>

        </div> --}}

        {{/each}}
      </div>
    </div>
  </section>


  {{!-- banner2 section --}}

  <section class="banner2_section py-5">
    <div class="container">
      <div class="row">
        <div class="col-md-6 text-center pt-5">
          {{!-- left side --}}
          <h3>
            we work for people,note for money
          </h3>
          <p>
            Lorem ipsum dolor sit amet consectetur, adipisicing elit. Omnis inventore optio maiores molestias autem
            laudantium.
          </p>
          <button class="btn btn-primary">Click Here</button>
        </div>
        <div class="col-md-6 text-center pt-2">
          {{!-- right side --}}
          <img src="/static/images/S3.png" alt="" style="max-height:350px;" />

        </div>
      </div>
    </div>
  </section>

  {{!-- banner3 section --}}


  <section class="banner2_section py-5">
    <div class="container">
      <div class="row">
        <div class="col-md-6 text-center pt-2">
          {{!-- left side --}}
          <img src="/static/images/S3.png" class="img-fluid" alt="" style="max-height:350px;" />

        </div>
        <div class="col-md-6 text-center pt-5">
          {{!-- right side --}}
          <h3>
            we work for people,note for money
          </h3>
          <p>
            Lorem ipsum dolor sit amet consectetur, adipisicing elit. Omnis inventore optio maiores molestias autem
            laudantium.
          </p>
          <button class="btn btn-primary">Click Here</button>
        </div>

      </div>
    </div>
  </section>

  {{!-- Contact us section --}}

  <section id="contact_us_section" class="py-5 background1">
    <div class="container">
      <h3 class="text-center text-secondary">Contact us</h3>
      <div class="row">
        <div class="col-md-12">

          <form action="/process-contact-form" method="post">
            <div class="mb-3">
              <label for="exampleInputEmail1" class="form-label">Your email address</label>
              <input name="email" type="email" class="form-control" id="exampleInputEmail1"
                aria-describedby="emailHelp">
              <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
            </div>
            <div class="mb-3">
              <label for="contactNumberField" class="form-label">Your contact number</label>
              <input name="phone" type="number" class="form-control" id="contactNumberField">
            </div>

            <div class="form-floating my-2">
              <textarea name="query" style="height:250px" class="form-control"
                placeholder="Enter your query here"></textarea>
              <label for="queryField">Your query</label>
            </div>


            <div class="container text-center">
              <button type="submit" class="btn btn-dark">Submit</button>
            </div>

          </form>

        </div>
      </div>
    </div>
  </section>

  {{!-- about section --}}

  <section class="about_section py-5" id="about_section">
    <div class="container text-center">
      <div class="row">
        <div class="col-md-12">
          <h3>About this side</h3>
          <p>Lorem ipsum dolor sit amet consectetur, adipisicing elit. Ad eaque corporis quod quam nulla quos repellat
            itaque minima blanditiis assumenda.</p>
          <buttion class="btn btn-success">
            Contact Us
          </buttion>
        </div>
      </div>
    </div>
  </section>


  <footer class="footer background1 text-center py-5">

    <div class="container">
      <div class="row">
        <div class="col-md-12">

          @ 2023 Copyright: <span>learncodewithrahul</span>


        </div>
      </div>
    </div>

  </footer>


  <!-- Optional JavaScript; choose one of the two! -->

  <!-- Option 1: Bootstrap Bundle with Popper -->
